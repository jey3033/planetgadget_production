<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <bannerslider>
            <general>
                <enabled>0</enabled>
                <module>Kemana_Banner</module>
            </general>
            <bannerslider_design>
                <responsive>0</responsive>
                <autoWidth>0</autoWidth>
                <autoHeight>1</autoHeight>
                <loop>1</loop>
                <nav>0</nav>
                <dots>0</dots>
                <lazyLoad>0</lazyLoad>
                <center>0</center>
                <autoplay>1</autoplay>
                <autoplayTimeout>5000</autoplayTimeout>
            </bannerslider_design>
        </bannerslider>
    </default>
</config>
