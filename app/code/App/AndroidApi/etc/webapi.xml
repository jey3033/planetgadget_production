<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route method="GET" url="/V1/api/getpost">
		<service class="App\AndroidApi\Api\PostManagementInterface" method="getPost"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
	<route method="GET" url="/V1/api/getproduct">
		<service class="App\AndroidApi\Api\PostManagementInterface" method="getProduct"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
</routes>